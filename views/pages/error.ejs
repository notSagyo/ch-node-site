<%- include('../partials/head.ejs', { pageTitle: 'Error' }) %>
<div class="container">
  <div class="col-md-6 offset-md-3 p-3 my-3">
    <div class="alert alert-danger d-flex flex-column" role="alert">
      <h1 class="mx-auto"><%= locals.errorTitle ? errorTitle : 'Unknown error' %> </h1>
      <span class="mx-auto"><%= locals.errorDescription ? errorDescription : '' %></span>
      <button class="btn btn-danger mx-auto mt-3" onclick="history.back()">Go back</button>
    </div>
  </div>
</div>
</html>
