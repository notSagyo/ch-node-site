{
  "name": "ch-node-site",
  "version": "1.0.0",
  "description": "Sitio desarrollado para la cursada de Backend de Coderhouse",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc --build --verbose",
    "preview": "pm2-runtime start ecosystem.config.js --env production",
    "start": "npm run preview",
    "watch": "nodemon --exec ts-node",
    "watchjs": "nodemon --exec node",
    "test": "ts-node src/tests/tests.ts",
    "fork": "pm2 start dist/app.js --name app8080 --watch -- --port 8080",
    "cluster": "pm2 start dist/app.js --name app8081 --watch -i max -- --port 8081"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/notSagyo/ch-node-site.git"
  },
  "author": "notSagyo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/notSagyo/ch-node-site/issues"
  },
  "homepage": "https://github.com/notSagyo/ch-node-site#readme",
  "dependencies": {
    "@faker-js/faker": "^7.4.0",
    "@firebase/firestore": "^3.4.12",
    "autocannon": "^7.9.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "express-session": "^1.17.3",
    "firebase": "^9.9.0",
    "knex": "^2.1.0",
    "log4js": "^6.6.1",
    "minimist": "^1.2.6",
    "mongoose": "^6.4.5",
    "multer": "1.4.5-lts.1",
    "mysql": "^2.18.1",
    "normalizr": "^3.6.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.2.0",
    "socket.io": "^4.5.1",
    "sqlite3": "^5.0.9",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/autocannon": "^7.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/minimist": "^1.2.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.6",
    "@types/passport": "^1.0.9",
    "@types/passport-local": "^1.0.34",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "ts-node": "^10.9.1"
  }
}
